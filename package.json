{
  "name": "knora-ui",
  "version": "8.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhlab-basel/Knora-ui.git"
  },
  "homepage": "https://dhlab-basel.github.io/Knora-ui",
  "license": "MIT",
  "keywords": [
    "angular",
    "knora",
    "digital humanities",
    "dhlab basel"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-docs": "./update_docs.sh",
    "build-lib": "rm -rf dist/'@knora'/ && ng build @knora/core && ng build @knora/action && ng build @knora/authentication && ng build @knora/viewer && ng build @knora/search",
    "build-lib-prod": "rm -rf dist/'@knora'/ && ng build @knora/core --prod && ng build @knora/action --prod && ng build @knora/authentication --prod && ng build @knora/viewer --prod && ng build @knora/search --prod",
    "yalc-publish": "yalc publish dist/@knora/action && yalc publish dist/@knora/authentication && yalc publish dist/@knora/core && yalc publish dist/@knora/search && yalc publish dist/@knora/viewer",
    "update-lib": "yarn install --prod=false && yarn build-lib",
    "build-yalc-action": "ng build @knora/action && yalc publish dist/@knora/action",
    "build-yalc-authentication": "ng build @knora/authentication && yalc publish dist/@knora/authentication",
    "build-yalc-core": "ng build @knora/core && yalc publish dist/@knora/core",
    "build-yalc-search": "ng build @knora/search && yalc publish dist/@knora/search",
    "build-yalc-viewer": "ng build @knora/viewer && yalc publish dist/@knora/viewer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/flex-layout": "7.0.0-beta.22",
    "@angular/forms": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@auth0/angular-jwt": "^2.0.0",
    "angular-split": "^3.0.1",
    "core-js": "^2.5.4",
    "global": "^4.3.2",
    "hammerjs": "^2.0.8",
    "jdnconvertiblecalendar": "^0.0.3",
    "jdnconvertiblecalendardateadapter": "^0.0.9",
    "json2typescript": "^1.0.6",
    "jsonld": "^1.0.2",
    "material-icons": "^0.3.1",
    "moment": "^2.22.2",
    "ngx-markdown": "^7.1.4",
    "node-sass": "^4.9.3",
    "openseadragon": "^2.4.0",
    "prismjs": "^1.15.0",
    "rxjs": "^6.2.2",
    "semver": "^6.1.1",
    "svg-overlay": "github:openseadragon/svg-overlay",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/prismjs": "^1.9.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-html-detailed-reporter": "^1.1.38",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "^0.0.32",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "3.2.4"
  }
}
